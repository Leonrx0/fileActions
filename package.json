{
  "name": "vue-file-actions",
  "version": "0.1.7",
  "description": "A lightweight Vue 3 file list + action component with support for delete, download, and upload operations.",
  "main": "dist/file-actions.umd.js",
  "module": "dist/file-actions.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/file-actions.es.js",
      "require": "./dist/file-actions.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "type-check": "tsc --noEmit",
    "dev": "vite",
    "demo": "vite --config example/vite.config.ts",
    "demo:build": "vite build --config example/vite.config.ts",
    "demo:preview": "vite preview --config example/vite.config.ts",
    "prepublishOnly": "node scripts/prepublish.js && npm run build"
  },
  "peerDependencies": {
    "element-plus": "^2.7.0",
    "lodash-es": "^4.17.21",
    "vue": "^3.4.0",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "element-plus": "^2.7.0",
    "typescript": "^5.5.0",
    "vite": "^5.3.0",
    "vue": "^3.4.0",
    "vue-tsc": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/file-actions.git"
  },
  "keywords": [
    "vue3",
    "file",
    "upload",
    "download",
    "component",
    "file-manager",
    "file-actions",
    "element-plus"
  ],
  "author": "leon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/your-username/file-actions/issues"
  },
  "homepage": "https://github.com/your-username/file-actions#readme",
  "dependencies": {
    "vue3-virtual-scroll-list": "^0.2.1"
  }
}
